{
  "name": "BusTrackerApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "android": "react-native run-android",
    "start": "react-native start",
    "test": "jest",
    "ts:check": "yarn tsc --noemit",
    "clean:android:gradlew": "cd android && ./gradlew clean && cd ..",
    "android:prod:build": "npm run clean:android:gradlew && npx react-native build-android --mode=release",
    "android:staging:build": "npm run clean:android:gradlew && cd android && ./gradlew assembleRelease && cd ..",
    "clean:ios:pods": "cd ios && rm -rf ./ios/Pods/** && npx pod-install && cd .."
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-native-community/geolocation": "^3.3.0",
    "@react-navigation/bottom-tabs": "^6.6.0",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.10.0",
    "axios": "^1.7.4",
    "babel-plugin-module-resolver": "^5.0.2",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-leaflet": "^4.2.1",
    "react-native": "0.74.3",
    "react-native-gesture-handler": "^2.17.1",
    "react-native-leaflet-view": "^0.1.2",
    "react-native-maps": "^1.15.6",
    "react-native-maps-directions": "^1.9.0",
    "react-native-reanimated": "^3.14.0",
    "react-native-safe-area-context": "^4.10.8",
    "react-native-screens": "^3.32.0",
